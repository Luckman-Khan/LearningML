    "import os\n",
    "os.environ[\"OMP_NUM_Threads\"] = \"1\"\n",
    "\n",
    "from sklearn import metrics\n",
    "from sklearn.cluster import KMeans\n",
    "from scipy.spatial.distance import cdist\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 23,
   "id": "6c207f91-60d7-475e-b669-d243dda6c27d",
   "metadata": {},
   "outputs": [],
   "source": [
    "x1 = np.array([3, 1, 1, 2, 1, 6, 6, 6, 5, 6, 7, 8, 9, 8, 9, 9, 8])\n",
    "x2 = np.array([5, 4, 5, 6, 5, 8, 6, 7, 6, 7, 1, 2, 1, 2, 3, 2, 3])\n",
    "X = np.array(list(zip(x1,x2)))\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 27,
   "id": "580f6482-603c-4cbb-bad7-c7a46e26292f",
   "metadata": {},
   "outputs": [],
   "source": [
    "distortions=[]\n",
    "inertias=[]\n",
    "map1={}\n",
    "map2={}\n",
    "\n",
    "K=range(1,10)\n",
    "for k in K:\n",
    "    KMeansModel = KMeans(n_clusters=k).fit(X)\n",
    "    \n",
    "    distortions.append(sum(np.min(cdist(X,KMeansModel.cluster_centers_,'euclidean'), axis=1,))/X.shape[0])\n",
    "    inertias.append(KMeansModel.inertia_)\n",
    "    map1[k] = sum(np.min(cdist(X,KMeansModel.cluster_centers_,'euclidean'),axis=1))/X.shape[0]\n",
    "    map2[k]= KMeansModel.inertia_\n",
    "\n",
    "    "
   ]
  },
  {
